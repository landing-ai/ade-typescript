{
  "dxt_version": "0.2",
  "name": "landingai-ade-mcp",
  "version": "2.0.1",
  "description": "The official MCP Server for the LandingAI ADE API",
  "author": {
    "name": "LandingAI ADE",
    "email": "support@landing.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/landing-ai/ade-typescript.git"
  },
  "homepage": "https://github.com/landing-ai/ade-typescript/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.landing.ai/",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "VISION_AGENT_API_KEY": "${user_config.VISION_AGENT_API_KEY}"
      }
    }
  },
  "user_config": {
    "VISION_AGENT_API_KEY": {
      "title": "apikey",
      "description": "Your unique API key for authentication.\n\nGet your API key here: https://va.landing.ai/settings/api-key.\n\nIf using the EU endpoint, get your API key here: https://va.eu-west-1.landing.ai/settings/api-key.",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
